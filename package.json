{
  "name": "timed-transition",
  "version": "1.1.0",
  "title": "TimedTransition",
  "description": "The simple shim for Transition Events with CSS transitions, that transparently intercepts difference between browsers.",
  "keywords": [
    "shim",
    "polyfill",
    "transition",
    "TransitionEvent",
    "timer",
    "transitionrun",
    "transitionstart",
    "transitionend",
    "transitioncancel"
  ],
  "main": "timed-transition.min.js",
  "module": "./src/timed-transition.js",
  "jsnext:main": "./src/timed-transition.js",
  "files": [
    "timed-transition.min.js",
    "src/*.js",
    "!src/*.proc.*",
    "webpack.config.rules.js",
    "bower.json",
    "LICENSE-MIT"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.3",
    "cssprefix": "^2.0.10",
    "jasmine-core": "^2.99.1",
    "log4js": "^2.5.3",
    "m-class-list": "^1.1.4",
    "node-static-alias": "^1.0.0",
    "pre-proc": "^0.2.8",
    "pre-proc-loader": "^1.0.9",
    "skeleton-loader": "^1.1.5",
    "stats-filelist": "^1.0.1",
    "test-page-loader": "^1.0.6",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack --display-modules",
    "test": "node ./test/httpd"
  },
  "homepage": "https://github.com/anseki/timed-transition",
  "repository": {
    "type": "git",
    "url": "git://github.com/anseki/timed-transition.git"
  },
  "bugs": "https://github.com/anseki/timed-transition/issues",
